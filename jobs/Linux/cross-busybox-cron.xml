<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Run the &lt;code&gt;cross-busybox&lt;/code&gt; job multiple times,&lt;br&gt;&#xd;
using the list of &lt;code&gt;BUILD_ARCH&lt;/code&gt; parameters:&#xd;
&lt;ul&gt;&#xd;
  &lt;li&gt;arm&lt;/li&gt;&#xd;
  &lt;li&gt;ppc&lt;/li&gt;&#xd;
&lt;/ul&gt;</description>
  <logRotator class="hudson.tasks.LogRotator">
    <daysToKeep>-1</daysToKeep>
    <numToKeep>10</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.googlecode.GoogleCodeProjectProperty plugin="googlecode@1.7"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>BUSYBOX_VERSION</name>
          <description>Version of busybox to build</description>
          <defaultValue>1.22.1</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty plugin="disk-usage@0.23"/>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec># build daily
@midnight</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>### CURL BUSYBOX -&gt; BUILD_ARCH = ARM ###
# build the URL from back to front
URL=&apos;TARBALL_DIR=&amp;#36;HOME&amp;#47;source&apos;
URL=&quot;BUILD_ARCH=arm&amp;PKG_VERSION=${BUSYBOX_VERSION}&amp;PKG_NAME=busybox&amp;${URL}&quot;
URL=&quot;https://shell.xaoc.org/jenkins/job/cross-busybox/buildWithParameters?${URL}&quot;
/usr/bin/wget \
  --spider \
  --no-check-certificate \
  --http-user=&quot;jenkcron&quot; \
  --http-password=&quot;TrawlWp7000&quot; \
  $URL
#  &quot;https://shell.xaoc.org/jenkins/job/cross-busybox/buildWithParameters?BUILD_ARCH=arm&amp;PKG_VERSION=${BUSYBOX_VERSION}&amp;PKG_NAME=busybox&amp;TARBALL_DIR=&amp;#36;HOME&amp;\#47;source&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>### CURL PREFLIGHT -&gt; BUILD_ARCH = ARM ###
URL=&apos;TARBALL_DIR=&amp;#36;HOME&amp;#47;source&apos;
URL=&quot;BUILD_ARCH=arm&amp;PKG_VERSION=${BUSYBOX_VERSION}&amp;PKG_NAME=busybox&amp;${URL}&quot;
URL=&quot;https://shell.xaoc.org/jenkins/job/cross-busybox/buildWithParameters?${URL}&quot;
/usr/bin/wget \
  --spider \
  --no-check-certificate \
  --http-user=&quot;jenkcron&quot; \
  --http-password=&quot;TrawlWp7000&quot; \
  $URL
# &quot;https://shell.xaoc.org/jenkins/job/cross-busybox/buildWithParameters?BUILD_ARCH=ppc&amp;PKG_VERSION=${BUSYBOX_VERSION}&amp;PKG_NAME=busybox&amp;TARBALL_DIR=&amp;\#36;HOME&amp;\#47;source&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>