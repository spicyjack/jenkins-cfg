<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Cairo library&lt;br&gt;&#xd;
Downloads: &lt;a href=&quot;http://www.cairographics.org/releases&quot;&gt;&#xd;
http://www.cairographics.org/releases&lt;/a&gt;</description>
  <logRotator class="hudson.tasks.LogRotator">
    <daysToKeep>-1</daysToKeep>
    <numToKeep>5</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.googlecode.GoogleCodeProjectProperty plugin="googlecode@1.7"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>PKG_NAME</name>
          <description>Name of the download tarball and source directory</description>
          <defaultValue>cairo</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>PKG_VERSION</name>
          <description>Version of libcairo to build</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>1.12.14</string>
              <string>1.12.12</string>
              <string>1.12.10</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>TARBALL_DIR</name>
          <description>Directory for downloaded source</description>
          <defaultValue>$HOME/source</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/update_jenkins_cfg.sh \
  --path $HOME/src/jenkins/config.git</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/delete_dir.sh \
  --path $WORKSPACE/output
/bin/sh $HOME/src/jenkins/config.git/scripts/delete_dir.sh \
  --path $WORKSPACE/artifacts
/bin/rm -f $WORKSPACE/libpixman.artifact.tar.xz</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/deps_check.sh \
  --deps &quot;libx11-dev&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/download_tarball.sh \
  --file ${PKG_NAME}-${PKG_VERSION}.tar.xz \
  --url http://www.cairographics.org/releases \
  --log $WORKSPACE/wget.lib${PKG_NAME}.log \
  --outdir $TARBALL_DIR</command>
    </hudson.tasks.Shell>
    <hudson.plugins.copyartifact.CopyArtifact plugin="copyartifact@1.27">
      <project>libcairo-dep-libpixman</project>
      <filter>pixman.artifact.tar.xz</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/unpack_artifacts.sh \
 -- pixman

/bin/bash $HOME/src/jenkins/config.git/scripts/munge_rpath.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>PKG_CONFIG_PATH=${WORKSPACE}/artifacts/lib/pkgconfig \
  /bin/sh $HOME/src/jenkins/config.git/scripts/unpack_and_config.sh \
  --prefix=${WORKSPACE}/output \
  --tarball=$TARBALL_DIR/${PKG_NAME}-${PKG_VERSION}.tar.xz \
  --config-args=&quot;--enable-gobject=yes&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>## BUILD AND TEST LIBCAIRO
cd ${PKG_NAME}-${PKG_VERSION}
time make
time make test TARGETS=xlib,xlib-render-0_0
time make install</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/bin/sh $HOME/src/jenkins/config.git/scripts/create_build_artifact.sh \
  --name=lib${PKG_NAME} \
  --source-version=${PKG_VERSION} \
  --output=${WORKSPACE}</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>libcairo.artifact.tar.xz</artifacts>
      <latestOnly>false</latestOnly>
      <allowEmptyArchive>false</allowEmptyArchive>
    </hudson.tasks.ArtifactArchiver>
  </publishers>
  <buildWrappers/>
</project>